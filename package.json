{
  "name": "trustlist",
  "version": "1.0.0",
  "description": "A curated list of company trust centers and compliance documentation",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node generate-utils.js && node generate-json.js && next build",
    "start": "next start",
    "lint": "next lint",
    "export": "npm run build && touch out/.nojekyll",
    "deploy": "npm run export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy\" && git subtree push --prefix out origin gh-pages",
    "generate": "node generate-json.js"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "fuse.js": "^7.0.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "trust-center",
    "compliance",
    "security",
    "privacy",
    "governance",
    "certifications"
  ],
  "homepage": "https://trustlists.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/FelixMichaels/TrustLists.git"
  },
  "license": "GPL-3.0"
}
